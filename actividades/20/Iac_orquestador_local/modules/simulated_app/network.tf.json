{
  "variable": [
    {
      "name": [
        {
          "type": "string",
          "default": "name-example-app",
          "description": "Nombre de la app que se usara como servidor local"
        }
      ]
    },
    {
      "network": [
        {
          "type": "string",
          "default": "lab-net",
          "description": "Nombre de la red local"
        }
      ]
    },
    {
      "port": [
        {
          "type": "number",
          "default": "8000",
          "description": "Puerto del servidor"
        }
      ]
    },
    {
      "api_key": [
        {
          "type": "string",
          "default": "",
          "sensitive": true,
          "description": "Api key privada"
        }
      ]
    }
  ]
}